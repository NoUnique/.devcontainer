{
  "name": "${localWorkspaceFolderBasename}",
  "dockerComposeFile": "${localWorkspaceFolder}/docker-compose.yml",
  "initializeCommand": "${localWorkspaceFolder}/.devcontainer/initialize.sh",
  "postCreateCommand": "${containerWorkspaceFolder}/.devcontainer/postcreate.sh",
  "workspaceFolder": "/app/${localWorkspaceFolderBasename}",
  "service": "dev",
  "runServices": ["dev"],
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
      "bash": {
        "path": "/bin/bash"
      }
    }
  }
}
